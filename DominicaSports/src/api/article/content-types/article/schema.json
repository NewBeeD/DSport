      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    },
    "Author": {
      "type": "string",
      "required": true
    },
    "all_league": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::all-league.all-league"
    },
    "dfa_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-player.dfa-player"
    },
    "dfa_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-team.dfa-team"
    },
    "daba_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::daba-player.daba-player"
    },
    "daba_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::daba-team.daba-team"
    },
    "dava_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dava-player.dava-player"
    },
    "dava_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dava-team.dava-team"
    },
    "dna_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dna-player.dna-player"
    },
    "dna_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dna-team.dna-team"
    },
    "Headline": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ],
      "default": "No",
      "required": true
    },
    "Page_Headline": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ],
      "required": true,
      "default": "No"
    },
    "Body_Content": {
      "type": "string",
      "required": true
    }
