{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Article_Img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    },
    "Author": {
      "type": "string",
      "required": true
    },
    "all_league": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::all-league.all-league"
    },
    "dfa_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-player.dfa-player"
    },
    "dfa_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-team.dfa-team"
    },
    "Headline": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ],
      "default": "No",
      "required": true
    },
    "Page_Headline": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ],
      "required": true,
      "default": "No"
    },
    "Body_Content": {
      "type": "text",
      "required": true
    },
    "RichText": {
      "type": "blocks",
      "required": false
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "News",
        "Talking Tactics",
        "Feature",
        "Video",
        "Report",
        "Transfers",
        "Club News",
        "Beyond the Field"
      ],
      "default": "News",
      "required": true
    }
  }
}
