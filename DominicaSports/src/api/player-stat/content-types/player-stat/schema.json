{
  "kind": "collectionType",
  "collectionName": "player_stats",
  "info": {
    "singularName": "player-stat",
    "pluralName": "player-stats",
    "displayName": "Player_Stat",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dfa_player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-player.dfa-player"
    },
    "Season": {
      "type": "enumeration",
      "enum": [
        "a2018-2019",
        "a2019-2020",
        "a2020-2021",
        "a2021-2022",
        "a2022-2023",
        "a2024-2025"
      ],
      "required": true
    },
    "Matches_Played": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "Goals": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "Assists": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "Yellow_Cards": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "Red_Cards": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "all_league": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::all-league.all-league"
    },
    "dfa_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dfa-team.dfa-team"
    },
    "Profile_Pic": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
